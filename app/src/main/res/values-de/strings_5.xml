    <string name="help_block_type">
        <![CDATA[
        - <font color="#d1c8c8"><b>Abweisen</b></font>: <br>
        &emsp; Wie das Drücken der Ablehnen-Taste. <br>
        <br>
        - <font color="#d1c8c8"><b>Stummschalten</b></font>: <br>
        &emsp; Klingelt im Hintergrund, sodass der Anrufer den Eindruck hat, dass du nur beschäftigt bist und nicht abgenommen hast. <br>
        <br>
        - <font color="#d1c8c8"><b>Annehmen und auflegen</b></font><font color="#ffa500"><b>(experimentell)</b></font><br>
        &emsp; Um zu vermeiden, dass du zur Mailbox weitergeleitet wirst, kann das Verhalten je nach Anbieter variieren.
        ]]>
    </string>
    <string name="stir">STIR-Attestierung</string>
    <string name="stir_include_unverified">Unverifizierte einschließen</string>
